<p>
  <a id="back-category" href="#categories/<%= @article.categorySlug %>">
    Back to category
  </a>
</p>
<h4>
  <%= @article.categoryName %> >>
</h4>
<h1 class="article-title">
  <%= @article.name %>
</h1>
<p><%= @article.dateToDisplay %></p>
<p><%= @article.author %></p>
<h2>Content</h2>
<div id="article-content">
  <%= @article.content %>
</div>

